@model IEnumerable<CoreEmployeMVC.Models.Detail>

@{
    ViewData["Title"] = "TotalWorkHr";
}

<h1>TotalWorkHr</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DepartmentName)
            </th>
           
           
            
            <th>
                @Html.DisplayNameFor(model => model.Total_Of_Worked_Hours)
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DepartmentName)
            </td>
         
            
            <td>
                @Html.DisplayFor(modelItem => item.Total_Of_Worked_Hours)
            </td>
           
        </tr>
}
    </tbody>
</table>
